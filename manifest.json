{
    "$schema": "https://raw.githubusercontent.com/FLECS-Technologies/app-sdk/3.0.0/manifest.schema.json",
    "_schemaVersion": "3.0.0",
    "app": "",
    "version": "##VERSION##",
    "revision": "0",
    "image": "flecs.azurecr.io/",
    "multiInstance": false,
    "editors": [
        {
            "name": "My UI",
            "port": 6789,
            "supportsReverseProxy": true
        }
    ],
    "args": [
        "--launch-arg-1",
        "--launch-arg-2=8000"
    ],
    "capabilities": [],
    "conffiles": [
        "default.conf:/etc/my-app/default.conf",
        "default.conf:/etc/my-app/default.conf:ro",
        "default.conf:/etc/my-app/default.conf:rw"
    ],
    "devices": [
        "/dev/net/tun"
    ],
    "env": [
        "MY_ENV=value"
    ],
    "ports": [
        "12345:12345"
    ],
    "volumes": [
        "my-app-etc:/etc/my-app",
        "/var/lib/my-app:/var/lib/my-app"
    ],
    "labels": [
        "org.my.label",
        "org.my.label-with-value=Some custom label value"
    ]
}
